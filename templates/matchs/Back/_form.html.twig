<div class="container " style="hight:100%">
                            {{ form_start(form) }}
                                <div class="form-group">
                                <label><h5>Home team<h5><label>
                                    {{ form_widget(form.EquipeHome , {'attr': {'class': 'form-control form-control-user' , 'placeholder' : 'Trounement Name'}} ) }}
                                    {{ form_errors(form.EquipeHome) }}
                                </div>
                                <div class="form-group">
                                <label><h5>Away team<h5><label>

                                  {{ form_widget(form.EquipeAway , {'attr': {'class': 'form-control form-control-user' , 'placeholder' : 'Equipe number  '}} ) }}
